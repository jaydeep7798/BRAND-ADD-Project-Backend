spring:
  application:
    name: SBMS-API_Gateway
  cloud:
      gateway :
        discovery :
           locator :
              enabled :true
              lower-case-service-id :true
        routes:
           - id: one
             uri: lb://SBMS-Welcome-API
             predicates:
              - Path=/welcome

           - id: two
             uri: lb://SBMS-Greet-API-Project
             predicates:
              - Path=/greet
         
        
        
   
         
    
server :
   port : 3333
   

